# focus window
alt - x: yabai -m window --focus recent
alt - h: yabai -m window --focus west
alt - j: yabai -m window --focus south
alt - k: yabai -m window --focus north
alt - l: yabai -m window --focus east

shift + alt - h: yabai -m window --swap west
shift + alt - j: yabai -m window --swap south
shift + alt - k: yabai -m window --swap north
shift + alt - l: yabai -m window --swap east

ctrl + alt - h: yabai -m window --stack west
ctrl + alt - j: yabai -m window --stack south
ctrl + alt - k: yabai -m window --stack north
ctrl + alt - l: yabai -m window --stack east

ctrl + shift + alt - h: yabai -m window --warp west
ctrl + shift + alt - j: yabai -m window --warp south
ctrl + shift + alt - k: yabai -m window --warp north
ctrl + shift + alt - l: yabai -m window --warp east

alt - n: yabai -m window --focus stack.next || yabai -m window --focus stack.first
alt - p: yabai -m window --focus stack.prev || yabai -m window --focus stack.last
cmd - pagedown: yabai -m window --focus stack.next || yabai -m window --focus stack.first
cmd - pageup: yabai -m window --focus stack.prev || yabai -m window --focus stack.last

alt - g: yabai -m window --toggle zoom-fullscreen
alt - z: yabai --restart-service

# send window to desktop and follow focus
shift + alt - q : yabai -m window --space 1 && yabai -m window --focus recent
shift + alt - w : yabai -m window --space 2 && yabai -m window --focus recent
shift + alt - e : yabai -m window --space 3 && yabai -m window --focus recent
shift + alt - r : yabai -m window --space 4 && yabai -m window --focus recent
shift + alt - a : yabai -m window --space 5 && yabai -m window --focus recent
shift + alt - s : yabai -m window --space 6 && yabai -m window --focus recent
shift + alt - d : yabai -m window --space 7 && yabai -m window --focus recent
shift + alt - f : yabai -m window --space 8 && yabai -m window --focus recent
